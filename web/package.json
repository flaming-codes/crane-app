    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "license-report": "^6.7.1",
    "msw": "^2.7.0",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
