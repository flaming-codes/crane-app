{
  "name": "web",
  "version": "1.4.1",
  "engines": {
    "node": ">=16.x"
  },
  "type": "module",
  "scripts": {
    "dev": "vite dev --open",
    "dev:prod": "npm run build && npm run preview -- --open",
    "build": "npm run gen:dep && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "api": "vite dev",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "gen:dep": "license-report --output=json > ./static/licenses.json",
    "gen:fakes": "npx ts-node --esm scripts/prepare.ts && npx ts-node --esm scripts/generate-packages.ts --count=100 && npx ts-node --esm scripts/generate-authors.ts --count=100",
    "gen:trends": "npx ts-node --esm scripts/generate-trends.ts --count=50"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@iconify/svelte": "2.2.1",
    "@playwright/test": "^1.29.1",
    "@sveltejs/adapter-vercel": "3.0.2",
    "@sveltejs/kit": "1.22.3",
    "@tailwindcss/forms": "0.5.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "18.7.18",
    "@types/yargs": "17.0.12",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "autoprefixer": "10.4.13",
    "clsx": "2.0.0",
    "dotenv": "16.3.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "license-report": "6.1.0",
    "npm-check-updates": "16.14.12",
    "postcss": "8.4.26",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "2.10.1",
    "svelte": "4.0.5",
    "svelte-check": "3.4.6",
    "svelte-confetti": "1.2.4",
    "svelte-markdown": "0.2.3",
    "svelte-meta-tags": "2.8.0",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.4.0",
    "tailwindcss-safe-area": "0.3.0",
    "ts-node": "10.9.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6",
    "vite": "4.4.4",
    "yargs": "17.7.2"
  },
  "dependencies": {
    "@lokidb/indexed-storage": "2.1.0",
    "@lokidb/loki": "2.1.0",
    "@rgossiaux/svelte-headlessui": "1.0.2",
    "comlink": "4.4.1",
    "date-fns": "2.30.0",
    "fuse.js": "6.6.2",
    "idb": "7.1.1",
    "idb-keyval": "6.2.1",
    "svelte-kit-bot-block": "0.0.5"
  }
}
