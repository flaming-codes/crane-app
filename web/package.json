{
  "name": "app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "2.8.0",
  "scripts": {
    "postinstall": "npm run licenses.build",
    "postuninstall": "npm run licenses.build",
    "build": "react-router build",
    "dev": "react-router dev",
    "preview": "npm run build && dotenv -- npm run start",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "update.all": "npx ncu -i --format group",
    "licenses.build": "license-report --output=json > ./app/licenses.json",
    "db.types": "npx supabase gen types typescript --local > app/data/db.types.generated.ts"
  },
  "dependencies": {
