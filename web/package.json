{
  "name": "app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "2.4.1",
  "scripts": {
    "postinstall": "npm run licenses.build",
    "postuninstall": "npm run licenses.build",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "preview": "npm run build && dotenv -- npm run start",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "update.all": "npm-check -u",
    "licenses.build": "license-report --output=json > ./app/licenses.json",
    "db.types": "npx supabase gen types typescript --local > app/data/db.types.generated.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.11",
    "@isaacs/ttlcache": "^1.4.1",
    "@mcansh/http-helmet": "^0.12.0",
    "@radix-ui/react-slot": "^1.1.1",
    "@remix-pwa/client": "^3.0.7",
    "@remix-pwa/sw": "^3.0.10",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@remixicon/react": "^4.6.0",
    "@resvg/resvg-js": "^2.6.2",
    "@splinetool/react-spline": "^4.0.0",
    "@supabase/supabase-js": "^2.47.10",
    "@uidotdev/usehooks": "^2.4.1",
    "ai": "^4.0.21",
    "clsx": "^2.1.1",
    "cva": "^1.0.0-beta.1",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.30.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.18",
    "minisearch": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.7.0",
    "satori": "^0.12.0",
    "schema-dts": "^1.1.2",
    "sonner": "^1.7.1",
    "tailwindcss-animated": "^1.1.2",
    "xss": "^1.0.15",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@remix-pwa/dev": "^3.1.0",
    "@remix-run/dev": "^2.15.2",
    "@remix-run/fs-routes": "^2.15.2",
