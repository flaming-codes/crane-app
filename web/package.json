{
  "name": "web",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.x"
  },
  "type": "module",
  "scripts": {
    "dev": "vite dev --open",
    "dev:prod": "npm run build && npm run preview -- --open",
    "build": "npm run gen:dep && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "api": "vite dev",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "gen:dep": "license-report --output=json > ./static/licenses.json",
    "gen:fakes": "npx ts-node --esm scripts/generate-packages.ts --count=100"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@iconify/svelte": "^2.2.1",
    "@playwright/test": "^1.26.0",
    "@sveltejs/adapter-vercel": "^1.0.0-next.78",
    "@sveltejs/kit": "^1.0.0-next.513",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "autoprefixer": "^10.4.12",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "license-report": "^6.1.0",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "svelte": "^3.51.0",
    "svelte-check": "^2.9.2",
    "svelte-media-queries": "^1.4.0",
    "svelte-meta-tags": "^2.6.3",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tailwindcss-safe-area": "^0.2.2",
    "ts-node": "^10.9.1",
    "tslib": "2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "yargs": "^17.6.0"
  },
  "dependencies": {
    "@lokidb/indexed-storage": "2.1.0",
    "@lokidb/loki": "2.1.0",
    "@rgossiaux/svelte-headlessui": "1.0.2",
    "comlink": "^4.3.1",
    "date-fns": "^2.29.3",
    "fuse.js": "^6.6.2",
    "idb": "7.0.2",
    "idb-keyval": "^6.2.0",
    "svelte-markdown": "^0.2.3"
  }
}
