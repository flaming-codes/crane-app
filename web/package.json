{
  "name": "app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "3.1.0",
  "mcpVersion": "1.1.0",
  "scripts": {
    "postinstall": "npm run licenses.build",
    "postuninstall": "npm run licenses.build",
    "build": "npm run build-storybook && react-router build",
    "dev": "react-router dev",
    "dev.mcp": "npx @modelcontextprotocol/inspector",
    "preview": "npm run build && dotenv -- npm run start",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "update.all": "npx ncu -i --format group",
    "licenses.build": "npx --yes license-report --output=json > ./app/licenses.json",
    "db.types": "npx supabase gen types typescript --local > app/data/db.types.generated.ts",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "build-storybook": "STORYBOOK=true storybook build -o public/storybook"
  },
